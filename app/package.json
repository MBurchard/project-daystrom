{
  "name": "skynet-app",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "typecheck:frontend": "vue-tsc --build --force",
    "typecheck:backend": "cargo check --manifest-path modules/backend/Cargo.toml",
    "test:frontend": "vitest run",
    "test:frontend:watch": "vitest",
    "test:frontend:coverage": "vitest run --coverage",
    "test:backend": "TS_RS_EXPORT_DIR=$PWD/modules/app/src/generated cargo test --manifest-path modules/backend/Cargo.toml",
    "test:backend:coverage": "TS_RS_EXPORT_DIR=$PWD/modules/app/src/generated cargo llvm-cov --manifest-path modules/backend/Cargo.toml",
    "dev": "TAURI_APP_PATH=./modules/backend tauri dev",
    "dev:frontend": "vite",
    "icons": "TAURI_APP_PATH=./modules/backend tauri icon resources/skynet.png",
    "build:frontend": "pnpm icons && vue-tsc --build && vite build",
    "preview:frontend": "vite preview",
    "tauri": "TAURI_APP_PATH=./modules/backend tauri"
  },
  "dependencies": {
    "@mburchard/bit-log": "^1.0.0",
    "@tauri-apps/api": "^2.10.1",
    "@tauri-apps/plugin-log": "^2.8.0",
    "pinia": "^3.0.4",
    "vue": "^3.5.28"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.10.0",
    "@types/node": "^24.10.13",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vitest/coverage-v8": "^4.0.18",
    "@vue/tsconfig": "^0.8.1",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.4"
  }
}
