{
  "$schema": "https://raw.githubusercontent.com/tauri-apps/tauri/dev/crates/tauri-utils/schema.json",
  "productName": "Project Daystrom",
  "version": "../../../package.json",
  "identifier": "mbur.project-daystrom",
  "build": {
    "frontendDist": "../app/dist",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "pnpm dev:frontend",
    "beforeBuildCommand": "pnpm build:frontend"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Project Daystrom",
        "width": 1024,
        "height": 768
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": {
      "../../resources/mod/libstfc-community-patch.dylib": "mod/libstfc-community-patch.dylib"
    }
  }
}
